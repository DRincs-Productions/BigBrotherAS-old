# game/scripts_s1/s1_v91.rpy:509
msgid "{i}перебраться в позу 69{/i}"
msgstr "{i}hacer 69{/i}"

# game/scripts_s1/s1_v91.rpy:509
msgid "другие позы"
msgstr "otras posiciones"

# game/scripts_s1/s1_v91.rpy:515
