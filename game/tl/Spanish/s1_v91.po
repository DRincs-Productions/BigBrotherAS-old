msgstr "{i}acariciar más suavemente con mi lengua{/i}"

# game/scripts_s1/s1_v91.rpy:1191
msgid "{i}следовать маминому совету{/i}"
msgstr "{i}Seguir el consejo de mamá{/i}"

# game/scripts_s1/s1_v91.rpy:1229
msgid "{i}сделать, как сказала мама{/i}"
msgstr "{i}hacer lo que dijo mamá{/i}"

# game/scripts_s1/s1_v91.rpy:1251
msgid "{i}освободить ванну{/i}"
msgstr "{i}Salir de la bañera{/i}"

# game/scripts_s1/s1_v91.rpy:1265
msgid "{i}дрочить, глядя на маму{/i}"
msgstr "{i}masturbáte mirando a mi madre{/i}"

# game/scripts_s1/s1_v91.rpy:1288
msgid "{i}прибраться и уйти{/i}"
msgstr "{i}limpia y vete{/i}"

# game/scripts_s1/s1_v91.rpy:1339
