msgstr "{i}acariciar con mi lengua sin prisas{/i}"

# game/scripts_s1/s1_v91.rpy:523
msgid "{i}кончить маме в рот{/i}"
msgstr "{i}correrme en la boca de mamá{/i}"

# game/scripts_s1/s1_v91.rpy:532
msgid "{i}продолжить ласкать языком{/i}"
msgstr "{i}seguir acariciando con mi lengua{/i}"

# game/scripts_s1/s1_v91.rpy:540
msgid "{i}ласкать языком активнее{/i}"
msgstr "{i}acariciar con la lengua con más vigor{/i}"

# game/scripts_s1/s1_v91.rpy:604
msgid "{i}натянуть шорты{/i}"
msgstr "{i}súbete los calzoncillos{/i}"

# game/scripts_s1/s1_v91.rpy:633
msgid "{i}уйти к себе в комнату{/i}"
msgstr "{i}ve a tu habitación{/i}"

# game/scripts_s1/s1_v91.rpy:753
msgid "{i}продолжить массаж с поцелуями в шею{/i}"
msgstr "{i}continuar el masaje con besos en el cuello{/i}"

# game/scripts_s1/s1_v91.rpy:768
msgid "{i}продолжить массаж и поцелуи{/i}"
msgstr "{i}continuar con el masaje y los besos{/i}"

# game/scripts_s1/s1_v91.rpy:796
msgid "{i}массировать мамину грудь{/i}"
msgstr "{i}masajear los pechos de mamá{/i}"

# game/scripts_s1/s1_v91.rpy:849
msgid "Нужна твоя помощь. Пойдём на диван, расскажу..."
msgstr "Necesito tu ayuda. Vamos al sofá. Te lo contaré..."

# game/scripts_s1/s1_v91.rpy:949
msgid "{i}идти на кухню{/i}"
msgstr "{i}ir a la cocina{/i}"

# game/scripts_s1/s1_v91.rpy:962
msgid "{i}пойти спать{/i}"
msgstr "{i}ir a la cama{/i}"

# game/scripts_s1/s1_v91.rpy:978
