msgstr "{i}Fermati da Alice{/i}"

# game/scripts_s1/s1_v92.rpy:170
msgid "Покрутись ещё немного..."
msgstr "Gira ancora un po'..."

# game/scripts_s1/s1_v92.rpy:170
msgid "{i}пойти оплатить{/i}"
msgstr "{i}Paga{/i}"

# game/scripts_s1/s1_v92.rpy:197
msgid "{i}идти домой{/i}"
msgstr "{i}Torna a casa{/i}"

# game/scripts_s1/s1_v92.rpy:264
msgid "{i}остаться с Алисой на ночь{/i}"
msgstr "{i}Rimani con Alice per la notte{/i}"

# game/scripts_s1/s1_v92.rpy:274
msgid "Эх... Хорошо, одевай."
msgstr "Eh... Va bene, indossalo."

# game/scripts_s1/s1_v92.rpy:274
msgid "Нет. Это уже слишком."
msgstr "No, questo è troppo."

# game/scripts_s1/s1_v92.rpy:284
msgid "{i}уйти спать в свою комнату{/i}"
msgstr "{i}Andare a dormire nella propria stanza{/i}"

# game/scripts_s1/s1_v92.rpy:306
msgid "{i}постараться проснуться пораньше{/i}"
msgstr "{i}Cerca di svegliarti presto{/i}"

# game/scripts_s1/s1_v92.rpy:316
msgid "{i}потрогать её грудь через одежду{/i}"
msgstr "{i}Toccare il seno attraverso i vestiti{/i}"

# game/scripts_s1/s1_v92.rpy:316
msgid "{i}стянуть лифчик с её груди{/i}"
msgstr "{i}Toglierle il reggiseno dal seno{/i}"

# game/scripts_s1/s1_v92.rpy:335
msgid "{i}потрогать её грудь{/i}"
msgstr "{i}Toccare il suo seno{/i}"

# game/scripts_s1/s1_v92.rpy:342
