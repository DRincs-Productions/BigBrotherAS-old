msgstr "{i}Spia Alice{/i}"

# game/scripts_s1/s1_v92.rpy:74
msgid "{i}идти в магазин одежды{/i}"
msgstr "{i}Andare nel negozio di abbigliamento{/i}"

# game/scripts_s1/s1_v92.rpy:106
msgid "{i}идти к примерочной{/i}"
msgstr "{i}Cammina fino al camerino{/i}"

# game/scripts_s1/s1_v92.rpy:112
msgid "{i}подсматривать за Алисой{/i}"
msgstr "{i}Sbircia Alice{/i}"

# game/scripts_s1/s1_v92.rpy:123
